{
    "name": "topoff/laravel-user-logger",
    "description": "Laravel User Logger",
    "keywords": [
        "laravel user logger"
    ],
    "homepage": "https://github.com/topoff/laravel-user-logger",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ndberg",
            "email": "code@bergerweb.ch"
        }
    ],
    "require": {
        "php": "^8.3",
        "jaybizzle/crawler-detect": "^1.2",
        "matomo/device-detector": "^6.3.0",
        "illuminate/support": "*",
        "illuminate/database": "*",
        "illuminate/cache": "*",
        "illuminate/log": "*",
        "laravel/pennant": "^1.0",
        "snowplow/referer-parser": "~0.2"
    },
    "_comments": {
        "snowplow/referer-parser": "Does not use semver and the newest version dev-master#2023-03-15-203816 does not get the php version. But the files seem to be ok, it uses the data/referers.json file from the repo."
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^2.1",
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^2.36",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Topoff\\LaravelUserLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Topoff\\LaravelUserLogger\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "clean" : "vendor/bin/rector && vendor/bin/pint && vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse",
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "rector": "vendor/bin/rector",
        "rector-dry": "vendor/bin/rector --dry-run",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Topoff\\LaravelUserLogger\\UserLoggerServiceProvider"
            ],
            "aliases": {
                "UserLogger": "Topoff\\LaravelUserLogger\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
